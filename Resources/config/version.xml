<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="zenstruck.version.manager.class">Zenstruck\VersionBundle\VersionManager</parameter>
        <parameter key="zenstruck.version.controller.class">Zenstruck\VersionBundle\Controller\VersionController</parameter>
    </parameters>

    <services>
        <service id="zenstruck.version.manager" class="%zenstruck.version.manager.class%">
            <argument />
        </service>

        <service id="zenstruck.version.controller" class="%zenstruck.version.controller.class%">
            <argument type="service" id="zenstruck.version.manager" />
        </service>
    </services>
</container>