<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>                
        <parameter key="zenstruck.version.data_collector.class">Zenstruck\VersionBundle\DataCollector\VersionDataCollector</parameter>
    </parameters>

    <services>
        <service id="zenstruck.version.data_collector" class="%zenstruck.version.data_collector.class%">            
            <tag name="data_collector" template="ZenstruckVersionBundle:Version:toolbar" id="version" />
            <argument /> <!-- file -->
            <argument /> <!-- text -->
        </service>
    </services>
</container>